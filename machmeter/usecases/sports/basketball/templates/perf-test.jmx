          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Create Team Player" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import com.google.cloud.spanner.*;
import java.util.*;
import com.google.cloud.Timestamp;

DatabaseClient dbClient = props.get(&quot;dbClient&quot;);
Timestamp gameCreatedAt = Timestamp.now();

dbClient
	.readWriteTransaction()
	.run(transaction -&gt; {
		String sql1 = &quot;Select id from player order by RAND() LIMIT 10&quot;;
		ResultSet resultSet = transaction.executeQuery(Statement.of(sql1));
		int[] players = new int[10];
		i = 0;
		while (resultSet.next()) {
			players[i++] = resultSet.getInt(&quot;id&quot;);
		}

		String sql1 = &quot;Select id from team where completed is not null LIMIT 1&quot;;
		ResultSet resultSet1 = transaction.executeQuery(Statement.of(sql1));
		int teamid;
		i = 0;
		while (resultSet1.next()) {
			teamid = resultSet.getInt(&quot;id&quot;);
		}
		
		int team_player_id = UUID.randomUUID().toInt();

		for (int playerid : players) 
		{ 
		    transaction.buffer(
			Mutation.newInsertBuilder(&quot;team_players&quot;)
				.set(&quot;id&quot;)
				.to(team_player_id)
				.set(&quot;player_id&quot;)
				.to(playerid)
				.set(&quot;team_id&quot;)
				.to(teamid)
				.build());
		}
		resultSet.close();
		resultSet1.close();
		}
	);

</stringProp>
          </JSR223Sampler>
