          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Write with Transactions in Protein" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import com.google.cloud.spanner.*;


DatabaseClient dbClient = props.get(&quot;dbClient&quot;);
String synthId = vars.get(&quot;synth_id&quot;);
String geneId = vars.get(&quot;GeneID&quot;);
String proteinId = vars.get(&quot;proteinId&quot;);
String proteinName = vars.get(&quot;proteinName&quot;);
String organism = vars.get(&quot;organism&quot;);

dbClient
	.readWriteTransaction()
	.run(transaction -&gt; {
		transaction.buffer(
			Mutation.newInsertBuilder(&quot;Protein&quot;)
				.set(&quot;synth_id&quot;)
				.to(synthId)
				.set(&quot;ProteinID&quot;)
				.to(proteinId)
				.set(&quot;ProteinName&quot;)
				.to(proteinName)
				.set(&quot;Organism&quot;)
				.to(organism)
				.build());
		}
	);

</stringProp>
