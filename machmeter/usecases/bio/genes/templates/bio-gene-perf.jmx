          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Read Classification and Interactions" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import com.google.cloud.spanner.*;


DatabaseClient dbClient = props.get(&quot;dbClient&quot;);

String geneId = vars.get(&quot;GeneID&quot;);
String readAllInteractionsSql = &quot;Select Localization, GeneID2, InteractionType, Expression_Corr from Classification as c, Interactions as i where c.GeneID = @geneId&quot;;
Statement stmt = Statement.newBuilder(readAllInteractionsSql)
      .bind(&quot;geneId&quot;).to(geneId)
      .build();

ResultSet result = dbClient
	.singleUseReadOnlyTransaction()
	.executeQuery(stmt);

String responseHeader = &quot;&quot;;
while (result.next()) {
	responseHeader += &quot;Localization : &quot; +result.getString(0);
	responseHeader += &quot;GeneID2 : &quot; +result.getString(1);
	responseHeader += &quot;InteractionType : &quot; +result.getString(2);
	responseHeader += &quot;Expression_Corr : &quot; +result.getBigDecimal(3);
}
result.close();

SampleResult.setRequestHeaders(responseHeader);</stringProp>
          </JSR223Sampler>
