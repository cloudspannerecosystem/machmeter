          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Write update Genes" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import com.google.cloud.spanner.*;
import java.util.*;


DatabaseClient dbClient = props.get(&quot;dbClient&quot;);
String synthId = vars.get(&quot;synth_id&quot;);

Mutation mutation = Mutation.newUpdateBuilder(&quot;Genes&quot;)
					.set(&quot;synth_id&quot;)
					.to(synthId)
					.set(&quot;Class&quot;)
					.to(&quot;centrons&quot;)
					.build();

ArrayList&lt;Mutation&gt; mutations = new ArrayList&lt;&gt;();
mutations.add(mutation);
dbClient.write(mutations);

SampleResult.setRequestHeaders(mutation.toString());</stringProp>
          </JSR223Sampler>
